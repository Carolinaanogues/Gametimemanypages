        <Rectangle HorizontalAlignment="Left" Height="54" Margin="177,335,0,0" Stroke="#FF154582" VerticalAlignment="Top" Width="50" Fill="#FF1C0B51"/>
        <Rectangle HorizontalAlignment="Left" Height="41" Margin="71,348,0,0" Stroke="#FF0A2C7D" VerticalAlignment="Top" Width="52" Fill="#FF1C0B51"/>

    </Grid>
